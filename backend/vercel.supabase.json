{
  "version": 2,
  "builds": [
    {
      "src": "supabase-entry.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "package.json",
          "supabase-entry.js",
          "config/**",
          "models/**",
          "routes/**",
          "controllers/**",
          "middleware/**",
          "utils/**",
          ".env"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "supabase-entry.js"
    }
  ],
  "env": {
    "DB_DIALECT": "postgres"
  }
}